<Application xmlns="https://github.com/avaloniaui"
			 xmlns:sty="using:FluentAvalonia.Styling"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="mclPlus.App">
    <Application.Styles>
        <sty:FluentAvaloniaTheme/>
    </Application.Styles>
	<Application.Resources>
		<ResourceDictionary>
			<FontFamily x:Key="icon">avares://mclPlus/assets/icon.ttf#Segoe MDL2 Assets</FontFamily>
			<FontFamily x:Key="default">avares://mclPlus/assets/font.ttf#MiSans</FontFamily>
		</ResourceDictionary>
	</Application.Resources>
</Application>
